#include<stdio.h>
int main()
{
    int n;
    float s,a,b;
    scanf("%d",&n);
    if(n<=199)
        s=n*1.20;
    else if(n>=200 && n<=400)
        s=n*1.50;
    else if(n>=400 && n<=600)
        s=n*1.80;
    else
        s=n*2.00;
    if(s>400)
        a=s*0.15;
    else
        a=100;
    b=s+a;
    printf("%.2f",b);

    return 0;
}
